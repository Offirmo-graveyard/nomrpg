<?xml version="1.0" encoding="UTF-8" ?>
<messages>
	
		<!-- to check if this rsrc file was correctly loaded (1/2) -->
	<message id="TBRPGTemplates.xml.begin">ok</message>

	<message id="TBRPG.default.play_area">
		<section OWAtemplate="TBRPG.default.play_area@TBRPGTemplates.xml">
			<div class="TBRPG_play_area panel resizable draggable">
				<div class="panel_helper_1">
					<article class="TBRPG_main_area">
						<div class="TBRPG_click_message" style="text-align: center;"><b>${result-txt}</b></div>
						<div class="TBRPG_button_area" style="text-align: center;">
							${countdown-wgt}
							${play-btn}
						</div>
					</article>
				</div>
			</div>
		</section>
	</message>
	

	<message id="TBRPG.default.countdown">
		<span OWAtemplate="TBRPG.stats_area@TBRPGTemplates.xml" id="countdown">${countdown-instructions}</span>
		<!--${countdown-progress-indicator}-->
	</message>


	<message id="TBRPG.default.stats_area">
		<section OWAtemplate="TBRPG.default.stats_area@TBRPGTemplates.xml">
			<div class="TBRPG_stats_area panel resizable draggable">
				<div class="panel_helper_1">
					<aside class="TBRPG_stats_area">
						<h3>${player-name-txt}</h3>
						<dl>
							<dt>${number-of-clicks-caption-txt}</dt><dd>${number-of-clicks-value-txt}</dd>
							<!-- more to come -->
						</dl>
					</aside>
				</div>
			</div>
		</section>
	</message>
	

	<message id="TBRPG.default.coming_soon">
<div class="coming_soon" OWAtemplate="TBRPG.default.coming_soon@TBRPGtemplates.xml">
	<p>Coming soon...</p>
</div>
	</message>


	<!-- to check if this rsrc file was correctly loaded (2/2) -->
	<message id="TBRPGTemplates.xml.end">ok</message>

</messages>
